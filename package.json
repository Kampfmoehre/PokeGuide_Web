{
  "name": "poke-guide-web",
  "version": "0.0.1",
  "description": "A pokedex made with nw.js and AngularJS",
  "main": "src/index.html",
  "scripts": {
    "test": "karma start",
    "test_watch": "karma start --auto-watch --no-single-run",
    "build": "rimraf dist/prod && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build_nw": "rimraf dist/dev && webpack --config config/webpack.dev.js --progress --profile --bail && nw.js/nw ./dist/dev/",
    "build_nw_no_gpu": "rimraf dist/dev && webpack --config config/webpack.dev.js --progress --profile --bail && nw.js/nw ./dist/dev/ --disable-gpu",
    "start": "rimraf dist/dev && webpack --config config/webpack.dev.js --progress --watch --bail",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kampfmoehre/PokeGuide_Web.git"
  },
  "keywords": [
    "pokedex",
    "pokemon"
  ],
  "author": "kampfmoehre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kampfmoehre/PokeGuide_Web/issues"
  },
  "homepage": "https://github.com/Kampfmoehre/PokeGuide_Web#readme",
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "typescript": "2.1.1",
    "typings": "^1.3.2",
    "webpack": "^1.13.0",
    "webpack-merge": "^0.14.0"
  }
}
