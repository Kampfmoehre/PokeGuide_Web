{
  "name": "poke-guide-web",
  "version": "0.0.1",
  "description": "A pokedex made with nw.js and AngularJS",
  "main": "src/index.html",
  "scripts": {
    "test": "karma start",
    "test_watch": "karma start --auto-watch --no-single-run",
    "build": "rimraf dist/prod && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build_nw": "rimraf dist/dev && webpack --config config/webpack.dev.js --progress --profile --bail && nw.js/nw ./dist/dev/",
    "build_nw_no_gpu": "rimraf dist/dev && webpack --config config/webpack.dev.js --progress --profile --bail && nw.js/nw ./dist/dev/ --disable-gpu",
    "start": "rimraf dist/dev && webpack --config config/webpack.dev.js --progress --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kampfmoehre/PokeGuide_Web.git"
  },
  "keywords": [
    "pokedex",
    "pokemon"
  ],
  "author": "kampfmoehre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kampfmoehre/PokeGuide_Web/issues"
  },
  "homepage": "https://github.com/Kampfmoehre/PokeGuide_Web#readme",
  "dependencies": {
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-merge": "^0.14.0"
  }
}
